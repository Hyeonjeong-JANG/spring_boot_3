{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberList}}
        <tr>
            <th scope="row">{{id}}</th>
            <td><a href="/members/{{id}}">{{email}}</a></td>
            <td colspan="2">{{password}}</td>
        </tr>
    {{/memberList}}
    </tbody>
</table>
<a href="/signup">회원가입</a>
{{>layouts/footer}}